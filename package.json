{
  "name": "toone-code",
  "displayName": "toone-code",
  "publisher": "Toone",
  "description": "Toone toonecode",
  "version": "0.0.2",
  "icon": "images/ai-logo.png",
  "engines": {
    "vscode": "^1.68.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Machine Learning",
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "menus": {
      "editor/context": [
        {
          "submenu": "toonecode-submenus",
          "group": "navigation"
        }
      ],
      "toonecode-submenus": [
        {
          "command": "toonecode.ask",
          "group": "tooneCode@1"
        },
        {
          "command": "toonecode.generateCode",
          "group": "tooneCode@2",
          "when": "editorHasSelection && generateCode-enabled"
        },
        {
          "command": "toonecode.addTests",
          "group": "tooneCode@3",
          "when": "editorHasSelection && addTests-enabled"
        },
        {
          "command": "toonecode.explain",
          "group": "tooneCode@4",
          "when": "editorHasSelection && explain-enabled"
        },
        {
          "command": "toonecode.addComments",
          "group": "tooneCode@5",
          "when": "editorHasSelection && addComments-enabled"
        }
      ]
    },
    "submenus": [
      {
        "id": "toonecode-submenus",
        "label": "TooneCode"
      }
    ],
    "keybindings": [
      {
        "command": "toonecode.new-completions",
        "key": "alt+p",
        "mac": "alt+p",
        "when": "editorFocus && !editorReadonly"
      }
    ],
    "commands": [
      {
        "command": "toonecode.ask",
        "title": "提问"
      },
      {
        "command": "toonecode.generateCode",
        "title": "生成代码",
        "enablement": "editorHasSelection"
      },
      {
        "command": "toonecode.addTests",
        "title": "生成测试",
        "enablement": "editorHasSelection"
      },
      {
        "command": "toonecode.optimize",
        "title": "优化建议",
        "enablement": "editorHasSelection"
      },
      {
        "command": "toonecode.explain",
        "title": "解析代码",
        "enablement": "editorHasSelection"
      },
      {
        "command": "toonecode.addComments",
        "title": "逐行注释",
        "enablement": "editorHasSelection && !editorReadonly"
      },
      {
        "command": "toonecode.disable-enable",
        "title": "Disable or enable TooneCode"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "toonecode-view-container",
          "title": "TooneCode",
          "icon": "images/ai-logo-in.svg"
        }
      ]
    },
    "views": {
      "toonecode-view-container": [
        {
          "type": "webview",
          "id": "toonecode.view",
          "name": ""
        }
      ]
    },
    "configuration": {
      "title": "toonecode",
      "properties": {
        "toonecode.EnableExtension": {
          "type": "boolean",
          "default": false,
          "description": "是否启用自动提示",
          "order": 7
        },
        "toonecode.promptPrefix.addTests": {
          "type": "string",
          "default": "为以上代码生成单元测试",
          "description": "允许您选中代码后，让 GPT 针对此代码提供测试的快速 Prompt 前缀。",
          "order": 8
        },
        "toonecode.promptPrefix.addTests-enabled": {
          "type": "boolean",
          "default": true,
          "description": "选中代码后，是否在上下文菜单选项显示[添加测试]",
          "order": 9
        },
        "toonecode.promptPrefix.explain": {
          "type": "string",
          "default": "解析以上代码",
          "description": "允许您选中代码后，让 GPT 针对此代码分析功能的快速 Prompt 前缀。",
          "order": 14
        },
        "toonecode.promptPrefix.explain-enabled": {
          "type": "boolean",
          "default": true,
          "description": "选中代码后，是否在上下文菜单选项显示[解释代码]",
          "order": 15
        },
        "toonecode.promptPrefix.addComments": {
          "type": "string",
          "default": "为以上每一行代码加上注释",
          "description": "允许您选中代码后，让 GPT 针对此代码添加注释的快速 Prompt 前缀。",
          "order": 16
        },
        "toonecode.promptPrefix.addComments-enabled": {
          "type": "boolean",
          "default": true,
          "description": "选中代码后，是否在上下文菜单选项显示[添加注释]",
          "order": 17
        },
        "toonecode.promptPrefix.completeCode": {
          "type": "string",
          "default": "补全以下的代码",
          "description": "允许您选中代码后，让 GPT 针对此代码补全代码的快速 Prompt 前缀。",
          "order": 18
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/isomorphic-fetch": "^0.0.39",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/uuid": "^9.0.8",
    "@types/vscode": "^1.68.0",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "delay": "^6.0.0",
    "esbuild": "^0.20.1",
    "eventsource-parser": "^1.1.2",
    "glob": "^10.3.10",
    "mocha": "^10.3.0",
    "p-timeout": "^6.1.2",
    "quick-lru": "^7.0.0",
    "ts-loader": "^9.5.1",
    "uuid": "^9.0.1"
  },
  "resolutions": {
    "clone-deep": "^4.0.1"
  }
}